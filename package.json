	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"format:check": "biome format --colors=force",
		"format:fix": "biome format --write",
		"lint:check": "biome lint --colors=force",
		"lint:fix": "biome lint --write",
		"test:dev": "vitest",
		"test:ci": "vitest run --reporter=dot --coverage",
		"typing:check": "tsc --noEmit",
		"security:check": "npm audit --audit-level=high",
		"security:fix": "npm audit fix --force",
		"storybook": "storybook dev -p 6006",
